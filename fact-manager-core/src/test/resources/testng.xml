<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="FactManager">
    <test name="FactManager" enabled="true" preserve-order="true">
        <groups>
            <run>
                <include name="facts"/>
                <include name="multipleFacts"/>
                <exclude name="pivotViewExporter"/>
            </run>
        </groups>
        <classes>
            <class name="com.biit.factmanager.test.FactsTests"/>
            <class name="com.biit.factmanager.test.PivotViewExporterTest"/>
            <class name="com.biit.factmanager.test.MultipleFactTests"/>
        </classes>
    </test>
</suite>
